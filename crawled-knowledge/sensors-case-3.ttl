@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dashb: <http://dynamicdashboard.ilabt.imec.be/broker/ontologies/dashboard#> .
@prefix metrics: <http://dynamicdashboard.ilabt.imec.be/broker/ontologies/metrics#> .

@base <http://localhost/web-thing-gateway/> .

<things/cosamira.module0.sensor_gnss_lat> rdfs:label "gnss_lat sensor (cosamira.module0)" ;
  sosa:observes <things/cosamira.module0.sensor_gnss_lat/properties/gnss_lat%3A%3Anumber> .
<things/cosamira.module0.sensor_gnss_lat/properties/gnss_lat%3A%3Anumber> rdfs:label "Latitude from gnss (cosamira.module0)" ;
  dashb:produces metrics:latitude .
<systems/cosamira.module0> ssn:hasSubSystem <things/cosamira.module0.sensor_gnss_lat> ;
  rdfs:label "cosamira.module0" .

<things/cosamira.module0.sensor_gnss_lat/sensors/sensor_gnss_lon> rdfs:label "gnss_lon sensor (cosamira.module0)" ;
  sosa:observes <things/cosamira.module0.sensor_gnss_lat/properties/gnss_lon%3A%3Anumber> .
<things/cosamira.module0.sensor_gnss_lat/properties/gnss_lon%3A%3Anumber> rdfs:label "Longitude from gnss (cosamira.module0)" ;
  dashb:produces metrics:longitude .
<systems/cosamira.module0> ssn:hasSubSystem <things/cosamira.module0.sensor_gnss_lat/sensors/sensor_gnss_lon> ;
  rdfs:label "cosamira.module0" .